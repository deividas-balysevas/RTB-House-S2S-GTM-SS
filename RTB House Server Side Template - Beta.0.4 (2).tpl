const sendHttpRequest = require('sendHttpRequest');
const getEventData = require('getEventData');
const getTimestampMillis = require('getTimestampMillis');
const makeString = require('makeString');
const encodeUriComponent = require('encodeUriComponent');
const logToConsole = require('logToConsole');

const taggingHash = makeString(data.advTaggingHash);
const partnerKey = makeString(data.partnerKey);
const userIdType = makeString(data.userIdType);
const userId = makeString(data[userIdType]);
const region = makeString(data.advRegion);
const timestamp = makeString(getTimestampMillis());

const pageLocation = getEventData('page_location') || '';
const pageReferrer = getEventData('page_referrer') || '';

const requestHeaders = { method: 'POST', timeout: 3000 };

const tagsRaw = [];
for (const key in data) {
    if (key.indexOf('tag') !== -1 && data[key]) {
        tagsRaw.push(key);
    }
}

const tagValue = '&tag=pr_' + taggingHash;
const tags = [];

tagsRaw.forEach((tag) => {

    let code = '';

    switch (tag) {

        case 'tagHome':
            code = tagValue + '_home';

            break;

        case 'tagCategory':
            const category = data.category ? makeString(data.category) : '';
            code = tagValue + '_category2_' + category;

            break;
        
        case 'tagSales':
            code = tagValue + '_sales';

            break;

        case 'tagNewOffers':
            code = tagValue + '_newoffers';

            break;

        case 'tagOffer':
            const offerId = data.offerOfferId ? makeString(data.offerOfferId) : '';
            code = tagValue + '_offer_' + offerId;

            break;

        case 'tagWishlist':
            const wishlistOfferId = data.wishlistOfferId ? makeString(data.wishlistOfferId) : '';
            code = tagValue + '_wishlist_' + wishlistOfferId;

            break;   
        
        case 'tagSize':
            const productSize = data.productSize ? makeString(data.productSize) : '';
            code = tagValue + '_size_' + productSize;

            break;

        case 'tagOfflineCheck':
            code = tagValue + '_offlinecheck';

            break;

        case 'tagListing':
            const listingOfferIds = data.listingOfferIds ? makeString(data.listingOfferIds) : '';
            code = tagValue + '_listing_' + listingOfferIds;

            break;

        case 'tagBasketAdd':
            const basketAddOfferId = data.basketAddOfferId ? makeString(data.basketAddOfferId) : '';
            code = tagValue + '_basketadd_' + basketAddOfferId;

            break;
      
        case 'tagBasket':
            code = tagValue + '_basket';

            break;

        case 'tagBasketStatus':
            const basketStatusOfferIds = data.basketStatusOfferIds ? makeString(data.basketStatusOfferIds) : '';
            code = tagValue + '_basketstatus_' + basketStatusOfferIds;

            break;

        case 'tagStartOrder':
            code = tagValue + '_startorder';

            break;

        case 'tagOrder':
            const orderSubclass = data.orderTagSubclassCheck ? makeString(data.orderTagSubClass) : 'purchase';
            const orderValue = data.orderValue ? makeString(data.orderValue).replace(',', '.') : '';
            const orderId = data.orderId ? makeString(data.orderId).split('_').join('-') : '';
            const orderOfferIds = data.orderOfferIds ? makeString(data.orderOfferIds) : '';
            const orderStatusCd = makeString(data.orderCd);
        
            code = tagValue +
              '_conversion_order_' +
              orderSubclass +
              '_' +
              orderValue +
              '_' +
              orderId +
              '_' +
              orderOfferIds +
              '&cd=' +
              orderStatusCd;

            break;
        
        case 'tagConversion':
            const conversionValue = data.conversionValueCheck ? makeString(data.conversionValue).replace(',', '.') : '';
            const conversionOrderId = data.conversionIdCheck ? makeString(data.conversionId).split('_').join('-') : '';
            const conversionOfferIds = data.conversionOfferIdsCheck ? makeString(data.conversionOfferIds) : '';
            const conversionCd = makeString(data.conversionCd);

            code = 
              tagValue + 
              '_conversion_' + 
              makeString(data.conversionClass) +
              '_' +
              makeString(data[data.conversionClass + 'SubClass']) +
              '_' +
              conversionValue + 
              '_' + 
              conversionOrderId + 
              '_' + 
              conversionOfferIds +
              '&cd=' +
              conversionCd;

            break;
        
        case 'tagPlacebo':
            code = tagValue;

            break;
        
        case 'tagConsent':
            code = tagValue + '_cnst_ads_0';
        
            break;

        case 'tagCustom':
            const customTagsTable = data.customTagsTable;
            customTagsTable.forEach((customTag) => {

                const customTagName = makeString(customTag.customTagName).split('_').join('-');
                const customTagValue = makeString(customTag.customTagValue);
                code += tagValue + '_custom_' + customTagName + '_' + customTagValue;

            });

            break;

    }

    tags.push(code);

});

const postbackUrl = 
      'https://' +
      region +
      '.creativecdn.com/partner/omni/postbacks?pkey=' +
      encodeUriComponent(partnerKey) +
      '&' +
      userIdType +
      '=' +
      encodeUriComponent(userId) +
      '&time=' +
      encodeUriComponent(timestamp) +
      tags.join('') +
      '&su=' +
      encodeUriComponent(pageLocation) +
      '&sr=' +
      encodeUriComponent(pageReferrer);

sendHttpRequest(postbackUrl, requestHeaders).then((result) => {
  if(result.statusCode < 200 || result.statusCode > 300) {
    data.gtmOnFailure();
  }
});

data.gtmOnSuccess();


